package jun;

public class jun {

	public static void main(String[] args) {
		int 숫자 = 1;
		int 숫자2 =1;
		
		while(숫자2<=10) { // 숫자=1, 숫자2=1, 1<=10(참)
			
			while(숫자<=10) { // 숫자=1, 숫자2=1 ,1<=10(참)
							// 2<=10(참) ... 10<=(참), 11<=10(거짓)
			System.out.println("*"); // **********
			숫자++;   //숫자=2,...10,11
		}
			숫자2++;  //숫자2=2...10,11
			
		

  }
	}
}
