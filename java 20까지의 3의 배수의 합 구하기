import java.util.Scanner;

public class Jun {

	public static void main(String[] args) {
		int sum = 0;
		Scanner sc = new Scanner(System.in);
		for(int i=1; 1<=20; i++) {
			if(i%3==0) {
				sum = sum + i;
				System.out.println("sum: "+sum);
		
			}
			System.out.println("최종 sum: " + sum);
		}
		
	}

}
