package jun17;

public class Hello {

	public static void main(String[] args) {
		System.out.println("안녕하세요"); 
		//syso 입력 후 crtl + space 누르고 엔터
	    //crtl + space 눌렀을때 바로뜨는경우 - 하나밖에 없어요 
	    //바로뜨지않는경우 - 여러개
		System.out.print("안녕하세요");

               
                //Alt + 방향키 줄바꿈
		System.out.println(3+5); //3+5의 결과를 출력하는문
		System.out.println("3+5"); //3+5를 그대로 출력하는문
		System.out.println("3+5="+(3+5)); //3+5=8을 출력하는문
	}

}
